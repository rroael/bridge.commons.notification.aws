<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <PackageId>Bridge.Commons.Notification.Aws</PackageId>
        <Title>Bridge.Commons.Notification.Aws</Title>
        <Version>1.0.0</Version>
        <Authors>Robson Leao</Authors>
        <Company>Bridge Core</Company>
        <Description>
            AWS Notification Package: Email, SMS, PushNotification etc.
        </Description>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <PackageReleaseNotes>First Version</PackageReleaseNotes>
        <PackageTags>bridge-core commons aws notification email sms push</PackageTags>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="AWSSDK.Core" Version="3.7.3.24"/>
        <PackageReference Include="AWSSDK.SimpleNotificationService" Version="3.7.2.55"/>
        <PackageReference Include="Bridge.Commons.Notification" Version="1.0.0"/>
        <PackageReference Include="Newtonsoft.Json" Version="13.0.1"/>
    </ItemGroup>

</Project>
